{
  "manifest_version": 3,
  "name": "URL Param Pruner",
  "description": "This extension removes specified URL parameters.",
  "version": "1.0",
  "permissions": ["activeTab", "tabs", "webNavigation", "storage"],
  "options_page": "options.html",
  "action": {
    "default_title": "Prune",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
